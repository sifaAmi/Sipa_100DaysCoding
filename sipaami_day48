public class day48{
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        System.out.println("\n=======");
        int[] nilai = {70, 90, 86, 95, 78};
        int total = 0;
        for (int b = 0; b < nilai.length; b++) {
            total += nilai[b];
        }

        System.out.println("Total: " + total);
    }
}
